{
  "name": "@plugjs/build",
  "version": "0.6.3",
  "description": "Simple shared build using PlugJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "bin": {
    "bootstrap-plugjs-build": "./dist/bootstrap.mjs"
  },
  "scripts": {
    "build": "plug",
    "coverage": "plug coverage",
    "dev": "plug coverage -w src -w test",
    "lint": "plug lint",
    "test": "plug test",
    "transpile": "plug transpile"
  },
  "peerDependencies": {
    "@plugjs/cov8": "^0.6.10",
    "@plugjs/eslint": "^0.6.10",
    "@plugjs/eslint-plugin": "^0.2.13",
    "@plugjs/expect5": "^0.6.10",
    "@plugjs/plug": "^0.6.10",
    "@plugjs/typescript": "^0.6.10"
  },
  "files": [
    "*.md",
    "dist/",
    "resources/",
    "resources/**/.*",
    "src/"
  ],
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/plugjs/build.git"
  },
  "keywords": [
    "build",
    "compile",
    "ecmascript",
    "javascript",
    "node",
    "plugjs",
    "transpile",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/plugjs/build/issues"
  },
  "homepage": "https://github.com/plugjs/build/#readme"
}
